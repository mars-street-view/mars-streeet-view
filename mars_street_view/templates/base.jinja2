<!DOCTYPE html>
<html>
  <!-- HEAD -->
  <head>
    <title>
      Mars Street View
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="../static/normalize.css">
    <link rel="stylesheet" type="text/css" href="../static/skeleton.css">
    <link rel="stylesheet" type="text/css" href="../static/base.css">
  </head>

  <!-- BODY -->
  <!-- =================================================================== -->
  <body>
    <section>

      <header class="top_header">
        <div>MARS STREET VIEW</div>
      </header>

      <section class='header'>

        <div class="logo">
          <img id="logo" src="../static/logo.png">
          <h1 id="title_header">MARS STREET VIEW</h1>
        </div>

        <nav class="nav">
          <ul>
            <li class="nav_item"><a href="">HOME</a></li>
            <li class="nav_item"><a href="">ROVER</a></li>
            <li class="nav_item"><a href="">HISTORY</a></li>
          </ul>
        </nav>
        <div class="space"></div>
        
      </section>

      <article class="container">
        
        <!-- BLOCK TO HOME JINJA -->
        {% block home %}
        {% endblock %}

        <!-- BLOCK TO ROVER JINJA -->
        {% block content %}
        {% endblock %}

        <!-- BLOCK TO HISTORY JINJA -->
        {% block history %}
        {% endblock %}
        
      </article>


      <footer class="footer">
        &copy; 2016 | Mars Street View 
      </footer>
    </section>
  </body>
</html>